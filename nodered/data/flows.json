    {
        "id": "d18c15afe8074504",
        "type": "function",
        "z": "2f58fda264849b1c",
        "name": "function 1",
        "func": "const deviceId = msg.topic.split(\"/\")[1];\nconst data = msg.payload;\n\nconst readings = {\n    query: `INSERT INTO readings (id, Device_id, humidity, unit, created_at) VALUES ($1, $2, $3, $4, $5)`,\n    params: [\n        data._id,\n        data.device_id,\n        data.humidity,\n        data.unit,\n        data.created_at = new Date()\n    ]\n}\n\nconst irrigationAlerts = null;\nif (data.humidity < 40) {\n    irrigationAlerts = {\n        collection: \"irrigation_alerts\",\n        operation: \"insertOne\",\n        payload: {\n            _id: data._id,\n            device_id: deviceId,\n            alert: \"HUMIDITY_LESS_THEN_40\",\n            created_at: new Date()\n        }\n    }\n}\n\nlet command = {\n    topic: `device/${deviceId}/cmd/pump`,\n    qos: 1,\n    retain: false,\n    payload: {\n        deviceId: deviceId,\n        light: null\n    }\n}\nconst currentsHours = new Date().getHours();\nif(data.humidity < 40 && ((currentsHours >= 5 && currentsHours < 9) || (currentsHours >=17 && currentsHours < 21))){\n    command.payload.light = \"on\";\n} else {\n    command.payload.light = \"off\";\n}\n\nreturn [readings, irrigationAlerts, command ];",
