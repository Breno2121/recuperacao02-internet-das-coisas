        "id": "4000212370bbf0f1",
        "type": "function",
        "z": "2f58fda264849b1c",
        "name": "ProcessInfoDevice",
        "func": "const deviceId = msg.topic.split(\"/\")[1];\n\nconst device = {\n    $set: {\n        model: msg.payload.model || null,\n        location: msg.payload.location || null,\n        light: msg.payload.light,\n        humidity: msg.payload.humidity,\n        update_at: new Date(),\n        created_at: new Date()\n    },\n    $setOnInsert: { \n        _id: msg.payload._id,\n        created_at:new Date ()\n    }\n}\n\nmsg.payload = [{ device_id: deviceId }, device, { upsert: true }];\n\nmsg.collection = \"devices\";\nmsg.operation = \"updateOne\";\n\nreturn msg;",
